<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-F3w7mX95PdgyTmZZMECAngseQB83DfGTowi0iMjiWaeVhAn4FJkqJByhZMI3AhiU" crossorigin="anonymous">
  <link rel="stylesheet" href="path/to/font-awesome/css/font-awesome.min.css">
    <title>Document</title>
</head>
<body>
    <form action="/se" method="post">
        <label for="">Enter your pin</label>
        <br>
        <input type="text" name="pin">
        <br>
        <label for="">Enter your bloodgroup</label>
        <br>
        <input type="text" name="bloodgroup">
        <button type="submit">Search</button>
    </form>
    <div class="container" style="margin-top: 5vh;">
        <table class="table">
          <thead class="table-dark">
            <tr>

              <th scope="col">Hospital</th>
              <th scope="col">Pincode</th>

              <th scope="col">Blood Group</th>
              <th scope="col">Quantity</th>
      
        
            </tr>
          </thead>
          <tbody>
              <% cd1.forEach(function(item){ %>
            <tr>
                  
              
      
              <th scope="row"><option  name="patientname"><%=item.hospital%></option></th>
              <td><option  name="patientname"><%=item.pincode%></option></td>
              <td><option  name="patientname"><%=item.blood%></option></td>

              <td><option  name="patientname"><%=item.quan%></option></td>

              <!-- <button></button> -->
           
              
          
      
            </tr>
              <% }) %>
          </tbody>
      
        </table>
        </div>
      
      
      
      
      
      
      
    
</body>
</html>