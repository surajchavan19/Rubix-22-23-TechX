<body>
   
    <div class="container" style="margin-top: 5vh;">
        <table class="table">
          <thead class="table-dark">
            <tr>
              <th scope="col">Blood Group</th>
              <th scope="col">Quantity</th>
      
        
            </tr>
          </thead>
          <tbody>
              <% cd1.forEach(function(item){ %>
            <tr>
                  
              
      
              <th scope="row"><option  name="patientname"><%=item.blood%></option></th>
         
               <!-- <button></button> -->
           
              <form action="/up" method="post">
                  <td> <input type="hidden" value="<%=item._id%>" name="ui"></td>
                  <td><input name="quantity" value="<%=item.quan%>"</td>   
              <td>  <button type="submit" class="btn btn-primary" data-toggle="modal" data-target="#exampleModalCenter"  >
                Update
                    </button></td>
                  </form>
          
      
            </tr>
              <% }) %>
          </tbody>
      
        </table>
        </div>
        <form action="/det" method="post">
          <label for="">Hname</label>
          <br>
            <input type="text" name="hospital">
            <br>
            <label for="">Pincode</label>
            <br>
    <input type="text" name="pincode" >
    <br>
    <label for="">Blood group</label>
    <br>
    <input type="text" name="blood">
    <br>
    <label for="">Quantity</label>
    <br>
    <input type="text" name="quan">
    <br>
    <!-- <input type="text" name="" id="">
     -->
     <button type="submit">submit</button>
    
        </form>
      
      

</body>
